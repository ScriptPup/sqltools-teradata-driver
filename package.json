{
  "name": "sqltools-teradata-driver",
  "displayName": "SQLTools Teradata Driver",
  "description": "VSCode SQLTools teradata driver",
  "version": "0.3.5",
  "engines": {
    "vscode": ">=1.61.0"
  },
  "publisher": "ScriptPup",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ScriptPup/sqltools-teradata-driver/"
  },
  "bugs": {
    "type": "git",
    "url": "https://github.com/ScriptPup/sqltools-teradata-driver/issues/"
  },
  "icon": "icons/package-icon.png",
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "package": "vsce package",
    "publish": "vsce publish --target win32-x64 linux-x64 darwin-x64",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./"
  },
  "keywords": [
    "teradata",
    "sqltools-driver",
    "sql"
  ],
  "categories": [
    "Programming Languages",
    "Snippets",
    "Formatters",
    "Other"
  ],
  "extensionDependencies": [
    "mtxr.sqltools"
  ],
  "activationEvents": [
    "onStartupFinished",
    "onLanguage:sql",
    "onCommand:sqltools.*"
  ],
  "main": "./out/extension.js",
  "dependencies": {
    "@sqltools/base-driver": "latest",
    "@sqltools/types": "latest",
    "ffi-napi": "^4.0.3",
    "keytar": "^7.7.0",
    "node-gyp": "^8.2.0",
    "ref-array-di": "^1.2.2",
    "teradata-nodejs-driver": "^1.0.0-rc.5",
    "uuid": "^7.0.2"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.5.5",
    "@types/node": "^14.0.9",
    "@types/vscode": "^1.42.0",
    "typescript": "^3.7.3"
  }
}
